    <!-- rtl -->
    <uses-permission android:name="com.vzw.APNPERMISSION" />
    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.MANAGE_ACCOUNTS" />
    <uses-permission android:name="android.permission.AUTHENTICATE_ACCOUNTS" />
    <uses-permission android:name="android.permission.READ_SYNC_SETTINGS" />
    <uses-permission android:name="android.permission.WRITE_SYNC_SETTINGS" />

    <!-- For Debugpanel -->
    <uses-permission android:name="android.permission.PROCESS_OUTGOING_CALLS" />

    <!-- GCM -->
    <permission
        android:name="com.verizon.messaging.vzmsgs.permission.C2D_MESSAGE"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.verizon.messaging.vzmsgs.permission.C2D_MESSAGE" />
    <uses-permission android:name="com.google.android.c2dm.permission.RECEIVE" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/vma_app_name" >

        <!-- Sync Service -->
        <service android:name="com.vzw.vma.sync.service.VMASyncService" >
            <intent-filter>
                <action android:name="vzm.sync.START" />
                <action android:name="vzm.msg.SEND" />
                <action android:name="vzm.sync.STOP" />
                <action android:name="vzm.mark.as.READ" />
                <action android:name="vzm.msg.DELETED" />
                <action android:name="vzm.xconv.DELETED" />
                <action android:name="vzm.sync.DELETE_OLD_MESSAGES" />
            </intent-filter>
        </service>
		 <service android:name="com.vzw.vma.sync.service.ProvisioningService" >
            <intent-filter>
                <action android:name="vzm.sync.START_HandsetProvisioning" />
                <action android:name="vzm.sync.STOP_HandsetProvisioning" />
                <action android:name="vzm.vma.publish.GCM_TOKEN" />
            </intent-filter>
        </service>
        <!-- GCM -->
        <service android:name="com.verizon.messaging.vzmsgs.GCMIntentService" />

        <receiver
            android:name="com.google.android.gcm.GCMBroadcastReceiver"
            android:permission="com.google.android.c2dm.permission.SEND" >
            <intent-filter>
                <action android:name="com.google.android.c2dm.intent.RECEIVE" />
                <action android:name="com.google.android.c2dm.intent.REGISTRATION" />

                <category android:name="com.verizon.messaging.vzmsgs" />
            </intent-filter>
        </receiver>
        <!-- GCM -->


        <!-- Application configuration -->
        <!-- VZM Provider -->
        <provider
            android:name="com.verizon.messaging.vzmsgs.provider.ApplicationProvider"
            android:authorities="vma"
            android:exported="false" />

        <meta-data
            android:name="com.vzmsgs.build"
            android:value="0" />
        <meta-data
            android:name="vzw.app.apn.enabled"
            android:value="false" />

        <!-- Test VZ Test MMSC Enabled -->
        <meta-data
            android:name="vma.host"
            android:value="vmaqaui.pdi.vzw.com" />
        <meta-data
            android:name="vma.imap.port"
            android:value="8143" />
    </application>

</manifest>