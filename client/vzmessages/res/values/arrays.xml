    <string-array name="all_smiley_texts">

        <!-- Tongue out -->
        <item>:-P</item>
        <item>:p</item>
        <item>:-p</item>
        <!-- Surprised -->
        <item>:-O</item>
        <item>:o</item>
        <item>:-o</item>
        <item>=-o</item>
